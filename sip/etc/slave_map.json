{
    "LTS": {
        "online": 1,
        "timeout": 10,
        "engine_url": "unix:///var/run/docker.sock",
        "launch_policy": "docker",
        "task_control_module": {
            "name": "TaskControlExample"
        },
        "docker_image": "sip",
        "task": [
            "sip/tasks/task.py",
            "2000"
            ]
        },
    "QA": {
        "online": 1,
        "timeout": 10,
        "engine_url": "unix:///var/run/docker.sock",
        "launch_policy": "docker",
        "task_control_module": {
            "name": "TaskControlExample"
        },
        "docker_image": "sip",
        "task": [
            "sip/tasks/task.py",
            "2000"
            ]
        },
    "pipeline": {
        "online": 0,
        "timeout": 10,
        "launch_policy": "spark",
        "task_control_module": {
            "name": "TaskControlSparkPoller",
						"timeout": 10.0
        },
        "task": {
					"jarfile": "sdp-pipeline_2.10-1.0.jar",
					"jarpath": "/usr/local/bin",
					"spark_args": "--conf spark.eventLog.enabled=true --conf spark.eventLog.dir=file:///tmp/spark-events --executor-memory 2g --driver-memory 2g --total-executor-cores 2"
				}
			},
    "ingest": {
        "online": 0,
        "timeout": 10,
        "engine_url": "unix:///var/run/docker.sock",
        "launch_policy": "docker",
        "task_control_module": {
            "name": "TaskControlExample"
        },
        "docker_image": "sip",
        "task": [
            "sip/tasks/exec_eng.py",
            "1000",
            "30"
            ]
        },
    "vis_receive": {
        "online": 0,
        "timeout": 10,
        "launch_policy": "docker",
        "task_control_module": {
            "name": "TaskControlProcessPoller",
            "timeout": 30.0
        },
        "docker_image": "sip",
        "task": [
            "#{sip_root}/tasks/vis_receiver_task.py",
            "#{sip_root}/etc/vis_receiver_config.json"
            ]
        },
    "pulsar_search": {
        "online": 0,
        "timeout": 10,
        "launch_policy": "docker",
        "task_control_module": {
            "name": "TaskControlProcessPoller",
            "timeout": 30.0
        },
        "docker_image": "sip",
        "task": [
            "#{sip_root}/tasks/pulsar_search_task.py",
            "#{sip_root}/etc/pulsar_receiver_config.json"
            ]
        }
}
